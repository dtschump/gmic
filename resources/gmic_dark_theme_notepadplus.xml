<NotepadPlus>
    <UserLang name="G&apos;MIC" ext="gmic">
        <Settings>
            <Global caseIgnored="no" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="yes" words3="yes" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">[00]00</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators">&apos; ! &quot; % &amp; ( ) * , / ; ? [ ] ^ ` { | } + &lt; = &gt;</Keywords>
            <Keywords name="Comment">1 2 0#</Keywords>
            <Keywords name="Words1">-if -else -elif -endif -repeat -done -local -endlocal -l --l -endl -do -while -continue -break -return : \</Keywords>
            <Keywords name="Words2">-</Keywords>
            <Keywords name="Words3">@ $</Keywords>
            <Keywords name="Words4"></Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="EAEAEA" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="BDAE9D" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="BDAE9D" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="B9CA4A" bgColor="282828" fontName="Consolas" fontStyle="1" fontSize="11" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="7AA6DA" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="8080FF" bgColor="282828" fontName="Consolas" fontStyle="3" fontSize="11" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="FFFFFF" bgColor="282828" fontName="Consolas" fontStyle="3" fontSize="11" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="408080" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="969896" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="C397D8" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="D5A064" bgColor="282828" fontName="Consolas" fontStyle="1" fontSize="11" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="D54E53" bgColor="282828" fontName="Consolas" fontStyle="3" fontSize="11" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="E78C45" bgColor="282828" fontName="Consolas" fontStyle="0" fontSize="11" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
